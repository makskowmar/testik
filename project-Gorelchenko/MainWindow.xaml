<Window x:Class="project_Gorelchenko.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:project_Gorelchenko"
        mc:Ignorable="d"
        Title="Авторизация" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Grid>
            <Grid.RowDefinitions>
                <RowDefinition Height="42"/>
                <RowDefinition/>
                <RowDefinition Height="42"/>
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition/>
            </Grid.ColumnDefinitions>
            <Rectangle Fill="#FFF4F4F5" StrokeThickness="0" HorizontalAlignment="Center" Height="42" Grid.Row="2" Stroke="Black" VerticalAlignment="Center" Width="800"/>
            <Button x:Name="ButtonBack" Content="Назад" HorizontalAlignment="Left" Margin="29,0,0,0" VerticalAlignment="Center" Height="42" Width="75" Click="Button_Click"  />
            <Frame x:Name="MainFrame" Grid.Row="1" Source="Pages/AuthPage.xaml" Navigated="MainFrame_OnNavigated"/>
        </Grid>

    </Grid>
</Window>
